<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="icon" format="reference"></attr>
    <attr name="color" format="color"></attr>
    <attr name="text" format="string"></attr>
    <attr name="text_size" format="dimension"></attr>

    <declare-styleable name="ChangeColorIconWithText">
        <attr name="icon"></attr>
        <attr name="color"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>
	<declare-styleable name="ScrollLayout">
        <attr name="labelerClass" format="string"/>
        <attr name="labelerFormat" format="string"/>
        <attr name="childWidth" format="dimension"/>
        <attr name="childHeight" format="dimension"/>
    </declare-styleable>
    
	<declare-styleable name="Gallery">
  
        <attr name="android:galleryItemBackground" />  
    </declare-styleable>  
    
     <declare-styleable name="LabelView">  
        <attr name="textColor" format="color" />  
        <attr name="textSize" format="dimension" />  
    </declare-styleable>  
</resources>