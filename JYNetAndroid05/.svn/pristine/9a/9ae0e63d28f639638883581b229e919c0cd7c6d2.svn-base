<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffff" >

    <include
        android:id="@+id/includeTop"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        layout="@layout/topbar" />

	<RelativeLayout
		android:id="@+id/laySearch"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_below="@id/includeTop"
		android:focusable="true"
		android:focusableInTouchMode="true" >

		<Button
			android:id="@+id/btnReset"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="2sp"
			android:layout_marginTop="2sp"
			android:layout_alignParentRight="true"
			android:text="@string/action_reset" />
		<Button
			android:id="@+id/btnSearch"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginBottom="2sp"
			android:layout_marginTop="2sp"
			android:layout_toLeftOf="@id/btnReset"
			android:text="@string/action_search" />
		<Button
			android:id="@+id/btnCode"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_toLeftOf="@id/btnSearch"
			android:layout_marginBottom="2sp"
			android:layout_marginRight="1sp"
			android:layout_marginTop="2sp"
			android:text="@string/action_flush" />

		<EditText
			android:id="@+id/etSearch"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:layout_marginBottom="2sp"
			android:layout_marginLeft="2sp"
			android:layout_marginTop="2sp"
			android:layout_toLeftOf="@id/btnCode"
			android:layout_alignParentLeft="true"
			android:hint="@string/tip_data"
			android:inputType="text" />
	</RelativeLayout>
	<ListView
		android:id="@+id/lvResult"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:cacheColorHint="#ffffff"
		android:divider="#000000"
		android:layout_below="@id/laySearch"
		android:layout_above="@+id/lB"

		>
	</ListView>
	<LinearLayout
		android:id="@+id/lB"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentBottom="true"
		android:layout_marginBottom="5sp"
		android:layout_margin="25dp"
		android:focusable="true"
		android:focusableInTouchMode="true"
		android:orientation="vertical" >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <Spinner
                android:id="@+id/gstate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:entries="@array/array_state_codes"
                android:prompt="@string/tip_state" />

            <Button
                android:id="@+id/state01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/gsimg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:text="@string/action_pictures" />

            <Button
                android:id="@+id/state02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1" />
        </LinearLayout>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"

	        android:orientation="horizontal"
	        >
			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:text="提箱地"
				/>
	        <EditText 
	            android:id="@+id/getboxspace"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:lines="1"
				android:gravity="center"
				android:inputType="text"
				android:background="@drawable/buttonwithline"
				android:imeOptions="actionNext"
				android:layout_marginRight="5dp"
	            />
			<TextView
				android:layout_width="0dp"
				android:layout_weight="1"
				android:layout_height="wrap_content"
				android:gravity="center"
				android:layout_marginLeft="5dp"
				android:text="换装车号"
				/>
	        <EditText 
	            android:id="@+id/transtid"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:lines="1"
				android:background="@drawable/buttonwithline"
				android:gravity="center"
				android:inputType="text"
				android:imeOptions="actionDone"
	            />
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        >
	        <TextView 
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="返回中方口岸时间"
	            />
	        <Button
	            android:id="@+id/backchnportime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="2"
				android:background="@drawable/buttonwithline"
				android:layout_marginRight="5dp"
                 />
	        <TextView 
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
				android:layout_marginLeft="5dp"
	            android:text="返回口岸库房时间"
	            />
	        <Button
	            android:id="@+id/backportstorehoustime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
				android:background="@drawable/buttonwithline"
                android:layout_weight="2"
                 />
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        >
	        <TextView 
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="口岸换装时间"
	            />
	        <Button
	            android:id="@+id/portranstime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="2"
				android:background="@drawable/buttonwithline"
				android:layout_marginRight="5dp"
                 />
	        <TextView 
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
				android:layout_marginLeft="5dp"
	            android:text="下线结费时间"
	            />
	        <Button
	            android:id="@+id/downlineovertime"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
				android:background="@drawable/buttonwithline"
                android:layout_weight="2"
                 />
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:orientation="horizontal"
	        >
			<TextView
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:text="铁路下线时间"
				/>
			<Button
				android:id="@+id/railwaydownlinetime"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:layout_weight="2"
				android:background="@drawable/buttonwithline"
				android:layout_marginRight="5dp"
				 />
			<TextView
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_weight="1"
				android:layout_marginLeft="5dp"
				android:text="实际回空时间"
				/>
			<Button
				android:id="@+id/fbacknulltime"
				android:layout_width="0dp"
				android:layout_height="wrap_content"
				android:layout_gravity="center_vertical"
				android:background="@drawable/buttonwithline"
				android:layout_weight="2"
				 />
	    </LinearLayout>
	<LinearLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginTop="20dp"
		android:background="@drawable/layoutwithline"
		android:orientation="horizontal"
		>
		<TextView
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:minHeight="50dp"
			android:layout_weight="1"
			android:gravity="center|center_horizontal|center_vertical"
			android:text="提箱时间"
			/>
		<LinearLayout
			android:layout_width="0dp"
			android:layout_weight="3"
			android:layout_marginRight="3dp"
			android:layout_marginTop="5dp"
			android:layout_marginBottom="5dp"
			android:layout_height="match_parent"
			android:background="#d6d6d6">
			<Button
				android:id="@+id/getboxtime"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@color/white"
				android:gravity="center_vertical|left"
				android:layout_marginLeft="1dp"
				android:textColor="#d6d6d6"
				android:text="   设置" />
		</LinearLayout>

		</LinearLayout>
		<Button
			android:id="@+id/btnOk"
			android:layout_width="match_parent"
			android:minHeight="40dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:background="#34BC5A"
			android:text="确   认"
			android:textSize="25dp"
			android:layout_marginTop="10dp"
			android:textColor="@color/white"
			/>
    </LinearLayout>
</RelativeLayout>